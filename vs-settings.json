{
  "workbench.iconTheme": "vscode-icons",
  "security.workspace.trust.untrustedFiles": "open",
  "git.enableSmartCommit": true,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.minimap.enabled": false,
  "vsicons.dontShowNewVersionMessage": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.zoomLevel": 2,
  "terminal.integrated.persistentSessionReviveProcess": "onExitAndWindowClose",
  "react-native-tools.showUserTips": false,
  "editor.inlayHints.fontFamily": "Courier New",
  "editor.inlayHints.fontSize": 11,
  "workbench.colorCustomizations": {
    "editorInlayHint.foreground": "#adadadf0",
    "editorInlayHint.background": "#3d3d3d48",
    "editorCursor.foreground": "#B1A2CA"
  },
  "workbench.colorTheme": "Default Dark+",
  "settingsSync.ignoredSettings": [],
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 1000,
  "vim.useSystemClipboard": true,
  "vim.smartRelativeLine": true,
  "vim.ignorecase": true,
  "vim.leader": "<space>",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "x"],
      "commands": ["workbench.action.moveEditorToNextGroup"]
    },
    {
      "before": ["<leader>", "r"],
      "commands": ["editor.action.rename"]
    },
    {
      "before": ["<leader>", "g", "d"],
      "commands": ["editor.action.peekDefinition"]
    },
    {
      "before": ["<leader>", "c", "a"],
      "commands": ["editor.action.quickFix"]
    },
    {
      "before": ["d"],
      "after": ["\"", "_", "d"]
    },
    {
      "before": ["D"],
      "after": ["\"", "_", "D"]
    },
    {
      "before": ["d", "d"],
      "after": ["\"", "_", "d", "d"]
    }
  ],
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "k"],
      "after": ["<Esc>"]
    },
    {
      "before": ["j", "k"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": ["<C-]>"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["<C-[>"],
      "commands": ["workbench.action.previousEditor"]
    },
    {
      "before": ["<leader>", "<leader>"],
      "after": ["z", "z"]
    },
    {
      "before": ["<leader>", "a"],
      "commands": ["vscode-harpoon.addEditor"]
    },
    {
      "before": ["<leader>", "e"],
      "commands": ["vscode-harpoon.editEditors"]
    },
    {
      "before": ["<leader>", "p", "e"],
      "commands": ["vscode-harpoon.editorQuickPick"]
    },
    {
      "before": ["<leader>", "1"],
      "commands": ["vscode-harpoon.gotoEditor1"]
    },
    {
      "before": ["<leader>", "2"],
      "commands": ["vscode-harpoon.gotoEditor2"]
    },
    {
      "before": ["<leader>", "3"],
      "commands": ["vscode-harpoon.gotoEditor3"]
    },
    {
      "before": ["<leader>", "4"],
      "commands": ["vscode-harpoon.gotoEditor4"]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["J"],
      "commands": ["editor.action.moveLinesDownAction"]
    },
    {
      "before": ["K"],
      "commands": ["editor.action.moveLinesUpAction"]
    },
    {
      "before": ["d"],
      "after": ["\"", "_", "d"]
    },
    {
      "before": ["D"],
      "after": ["\"", "_", "D"]
    },
    {
      "before": ["d", "d"],
      "after": ["\"", "_", "d", "d"]
    }
  ],
  "editor.lineNumbers": "relative"
}
